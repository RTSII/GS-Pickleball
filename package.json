{
  "name": "grand-strand-pickleball",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18 <21"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "testwatch": "npm run test:watch",
    "typecheck": "tsc --noEmit",
